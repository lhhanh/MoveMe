{
    "name": "MoveMe",
    "description": "A grumpy cat that stares at you until you move your body",
    "version": "1.0.0",
    "manifest_version": 3,

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }  
    ],

    "action": {
        "default_title": "MoveMe"
    },

    "permissions":["activeTab"],

    "content_security_policy": {}
  }